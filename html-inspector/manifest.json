{
    "manifest_version": 2,
    "name": "HTML Validator",
    "short_name" : "HTMLValidator",
    "version": "0.2.0",
    "description": "Validating web page",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon48.png"
    },
    "background": {
        "scripts": ["event.js"],
        "persistent": false
    },
    "permissions": ["tabs", "activeTab"]
}